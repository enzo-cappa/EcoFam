<tr id="<%= dom_id product %>">
  <td><%= product.name %></td>
  <td>
    <% product.annual_price_rise.each do |brand, percent| %>
    <p><%= brand.name %>: <%= number_to_percentage(percent, precision: 1) %></p>
    <% end %>
  </td>
  <td>
    <div class="btn-group">
      <a class="btn dropdown-toggle" data-toggle="dropdown" href="#"> Acci&oacute;n <span class="caret"></span> </a>
      <ul class="dropdown-menu">
         <li><%= link_to 'Precios', product_price_lines_path(product), remote: true %></li>
         <li><%= link_to 'Nuevo precio', new_product_price_line_path(product), remote: true %></li>
      </ul>
    </div>
  </td>
</tr>
