$('#chart').highcharts({
  title: {
    text: 'Precios en los Ãºltimos 6 meses',
    x: -20 //center
  },
  tooltip: {
    valuePrefix: '$ ',
    valueDecimals: 2
  },
  legend: {
    layout: 'vertical',
    align: 'right',
    verticalAlign: 'middle',
    borderWidth: 0
  },
  xAxis: {
    type: 'datetime'
  },
  series: [
      <% @data_by_market.keys.each_with_index do |name, i| %>
         <%= "," unless i == 0%>
         {
           name: "<%= name %>",
           data: [
         <% @data_by_market[name].each_with_index do |row, i| %>
           <%= "," unless i == 0%>
             [Date.UTC(<%= row[0].year %>, <%= row[0].month %>, <%= row[0].day %>), <%= row[1] %>]
           <% end %>
           ]
         }
      <% end %>
  ]
});

